{
  "expo": {
    "name": "CalendAgro",
    "slug": "CheckIn",
    "owner": "scailancrei",
    "version": "1.0.0",
    "web": {
      "favicon": "./assets/images/plant.png"
    },
    "experiments": {
      "tsconfigPaths": true
    },
    "plugins": [
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#232323",
          "image": "./assets/images/splash-icon.png",
          "dark": {
            "image": "./assets/images/splash-icon.png",
            "backgroundColor": "#000000"
          },
          "imageWidth": 200
        }
      ]
    ],
    "orientation": "portrait",
    "icon": "./assets/images/plant.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/splash-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.anonymous.CalendAgro"
    },
    "androidStatusBar": {
      "backgroundColor": "#0A48A5",
      "barStyle": "dark-content"
    },
    "extra": {
      "eas": {
        "projectId": "a2295686-530a-4039-b9e2-663563bed32b"
      }
    }
  }
}
